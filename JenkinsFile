pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
               
                	echo 'Starting to build pipeline to display vowels of a sentence in upper case'
                	sh 'mvn clean compile'
                 
            }
        }
        stage('Test') {
            steps {
            	
                	echo 'Testing stage....and now I have completed testing'
                	sh 'mvn test'
                
            }
        }
    }
}
